		background-color: #1976D2;
		color: #ffffff
	}

	table.blue-cv {
		border-spacing: 0;
		border-collapse: collapse;
		width: 100%;
		border: 1px solid #DCDCDC;
		font-family: open sans, SolaimanLipi
	}

	table.blue-cv .left-side {
		background-color: #F5F5F5;
		padding: 20px 30px 20px 30px;
		width: 42%;
		vertical-align: top
	}

	table.blue-cv .right-side {
		padding: 25px 0 25px 40px;
		width: 58%;
		vertical-align: top
	}

	table.t-l-pinfo {
		border-spacing: 0;
		border-collapse: collapse;
		width: 100%;
		table-layout: auto
	}

	table.t-l-pinfo h3 {
		margin-top: 10px;
		line-height: 1.1
	}

	table.t-l-pinfo td.t-a-c img {
		max-width: 124px;
		height: 131px;
		table-layout: auto
	}

	table.t-l-pinfo td.t-n-a-info-wrap {
		margin: 0 0 0 0;
		padding: 20px 0 15px 0
	}

	table.t-n-a-info {
		border-spacing: 0;
		border-collapse: collapse;
		width: 100%;
		font-size: 14px
	}

	table.t-n-a-info img {
		margin-right: 5px
	}

	table.t-n-a-info td,
	table.t-n-a-info a {
		color: #000000;
		font-weight: 600;
		text-decoration: none
	}

	table.t-n-a-info a {
		word-break: break-word
	}

	table.t-n-a-info td {
		padding-bottom: 10px;
		vertical-align: text-top
	}

	table.t-l-pinfo td.t-per-info-wrap {
		margin: 0 0 0 0;
		padding: 10px 0 10px 0
	}

	table.t-per-info {
		border-spacing: 0;
		border-collapse: collapse;
		width: 100%
	}

	table.t-per-info td {
		display: table-cell
	}

	table.t-per-info img {
		margin-right: 10px
	}

	table.t-per-info td.t-title {
		color: #2F64A3;
		font-size: 18px
	}

	table.t-per-info td.p-info-dtl {
		margin: 0 0 0 0;
